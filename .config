---
antden:
  scheduler:
#    db: $ROOT/database/scheduler.db
    db:
      database: 'antden:AntDen_DB_IP'
      user: root
      pass: antden123456^!
    code: $ROOT/scheduler/code
    conf: $ROOT/scheduler/conf
    temple: $ROOT/scheduler/temple
  controller:
    code: $ROOT/controller/code
    conf: $ROOT/controller/conf
  slave:
    db: $ROOT/database/slave.db
    code: $ROOT/slave/code
    conf: $ROOT/slave/conf
  simulator:
    scheduler: $ROOT/scheduler/temple
    conf: $ROOT/simulator/conf

agent:
  path: $MYDanPATH/dan/agent/path
  argv: $MYDanPATH/dan/agent/argv
  auth: $MYDanPATH/etc/agent/auth
  proxy: $MYDanPATH/etc/agent/proxy
  conf: $MYDanPATH/dan/agent/conf
  lib:  $MYDanPATH/dan/agent/lib
  port: 65111
  role: agent #myrole

dashboard:
  auth: $ROOT/dashboard/auth
